#routines.transitions-enabled
	.box.panel.panel-default
		%img{:src => "http://s9.postimg.org/g284ahusf/US_exercise_1.jpg"}
			%h2= link_to "New", new_routine_path(@routine)


	- @routines.each do |routine|
		.box.panel.panel-default
			= link_to (image_tag routine.image.url), routine
			%h2= link_to routine.title, routine


					
					
